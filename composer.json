{
    "name": "draperstudio/laravel-sweet-flash",
    "type": "library",
    "description": "Flash messages through SweetAlert for Laravel 5.",
    "keywords": [
        "draperstudio",
        "laravel-sweet-flash"
    ],
    "homepage": "https://github.com/DraperStudio/laravel-sweet-flash",
    "license": "MIT",
    "authors": [{
        "name": "DraperStudio",
        "email": "hello@draperstudio.tech",
        "homepage": "http://draperstudio.tech",
        "role": "Developer"
    }],
    "require": {
        "php": "~5.5|~7.0",
        "draperstudio/laravel-service-provider": "^1.0",
        "illuminate/session": "5.1.*|5.2.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.1.*|3.2.*",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "DraperStudio\\SweetFlash\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DraperStudio\\SweetFlash\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
